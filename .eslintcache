  \r\n\r\n            keyFrames.push(startingNum)\r\n        }\r\n    }\r\n    \r\n  \r\n    this.info = \r\n          <motion.div\r\n       \r\n                    whileHover={{y: keyFrames}}\r\n                    \r\n                    transition={{\r\n                        \r\n                        duration:1.45,\r\n                        \r\n                        \r\n                        \r\n                    }}>\r\n       \r\n       {Letter}\r\n  \r\n    </motion.div> \r\n   // console.log(` Keyframes: ${keyFrames}`)\r\n        }\r\n  \r\n    }\r\n\r\n\r\n\r\nfunction SocialMedia(){\r\n\r\n    let s = new Letter(\"S\")\r\n    let o = new Letter(\"o\")\r\n    let c = new Letter(\"c\")\r\n    let i = new Letter(\"i\")\r\n    let a = new Letter(\"a\")\r\n    let l = new Letter(\"l\")\r\n\r\n\r\n    let m = new Letter(\"M\")\r\n    let e = new Letter(\"e\")\r\n    let d = new Letter(\"d\")\r\n    //let i = new Letter(\"i\")\r\n    //let a = new Letter(\"a\")\r\n \r\n    \r\n    \r\n    return(\r\n\r\n        <div className=\"flex\" style={{textShadow: \"2px 2px #444444\"}}>\r\n\r\n            <div className=\"flex\">\r\n                \r\n                {s.info}\r\n                {o.info}\r\n                {c.info}\r\n                {i.info}\r\n                {a.info}\r\n                {l.info}\r\n\r\n            </div>\r\n\r\n                \r\n\r\n            <div className=\"flex ml-8\">\r\n\r\n                {m.info}\r\n                {e.info}\r\n                {d.info}\r\n                {i.info}\r\n                {a.info}\r\n                \r\n\r\n            </div>\r\n\r\n        </div>\r\n\r\n    )\r\n\r\n}\r\n\r\nexport default SocialMedia","C:\\Users\\Brandon\\Desktop\\React studies\\learning-app\\my-app\\src\\Components\\AboutUs.js",["104"],"\r\nimport React from 'react'\r\nimport Staff from './Staff'\r\n\r\n\r\nvar w = window.innerWidth;\r\nvar h = window.innerHeight\r\n\r\nvar JayArray = [\r\n\r\n    \"I first started drawing